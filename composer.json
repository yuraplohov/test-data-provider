{
    "name": "yuraplohov/test-data-provider",
    "description": "Providing test data for autotests from php files with returned arrays, and from any text files",
    "keywords": [
        "laravel",
        "tests",
        "unit",
        "test data",
        "api test",
        "phpunit",
        "codeception",
        "pest"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy PLokhov",
            "email": "yurii.plohov@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Yuraplohov\\TestDataProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuraplohov\\TestDataProvider\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
